#!/usr/bin/env ruby
require 'yaml'
require 'pgn'
require 'net/http'
require 'json'

require 'meta_repertoire'

repertoire_config = YAML.load(File.read(ARGV[0]))
repertoire = Repertoire.new(repertoire_config['color'], repertoire_config['lines'], repertoire_config['games'])

repertoire.build_lines
p repertoire.lines.first
